{
  "imports": {
    "@deno/sandbox": "jsr:@deno/sandbox@^0.4.1"
  },
  "tasks": {
    "shell": "deno run --env-file=.env -A examples/01_shell.ts",
    "files": "deno run --env-file=.env -A examples/02_files.ts",
    "processes": "deno run --env-file=.env -A examples/03_processes.ts",
    "repl": "deno run --env-file=.env -A examples/04_repl.ts",
    "http": "deno run --env-file=.env -A examples/05_http_server.ts",
    "all": "deno task shell && deno task files && deno task processes && deno task repl && deno task http"
  }
}
